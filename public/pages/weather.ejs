<section id="newsDetails">
  <div class="preloader-wrapper" ng-show="preloader">
    <h1 class="center-align">Loading</h1>
    <div class="progress">
      <div class="indeterminate"></div>
    </div>
  </div>
  <div class="container">    
    <h1 class="center-align">{{ msg }}</h1>
    <div class="row">
      <div class="card col s12 p-2 teal darken-2 white-text" ng-repeat="forecast in forecasts">
        <div class="card-title">
          <h4 class="center">{{ forecast.day }} {{ forecast.date | reverse }} in {{ city }} {{ region_name }}, {{ country_name }}</h4>
        </div>
        <div class="card-content">
          <div class="row">
            <div class="col s12 m6">
              <p class="center-align flow-text"><strong>High Temperature : </strong> {{ forecast.high }} &deg;C <br /> <strong>Low Temperature : </strong> {{ forecast.low }} &deg;C</p>
            </div>
            <div class="col s12 m6">
              <p class="center-align flow-text"><strong>Type of Day:</strong> {{ forecast.skytextday }}</p>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>